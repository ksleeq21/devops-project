{
    "kind":"ReplicationController",
    "apiVersion":"v1",
    "metadata":{
       "name":"webapp",
       "labels":{
          "app":"webapp"
       }
    },
    "spec":{
       "replicas":3,
       "selector":{
          "app":"webapp"
       },
       "template":{
          "metadata":{
             "labels":{
                "app":"webapp"
             }
          },
          "spec":{
             "containers":[
                {
                   "name":"webapp",
                   "image":"ksleeq21/devops-project:81b2ecbc0a474348031e9995eb56f06ede1553cd",
                   "ports":[
                      {
                         "name":"http-server",
                         "containerPort":8000
                      }
                   ]
                }
             ]
          }
       }
    }
 }